<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.guangyou.rareanimal.mapper.ActivityCoverMapper">

<!--    List<String> getActivityCoverById(Long activityId);-->
    <select id="getActivityCoverById" resultType="java.lang.String">
        select cover_url
        from t_activity_cover
        where id = #{activityId}
    </select>
</mapper>